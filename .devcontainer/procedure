quarto create-project myblog --type website
cd myblog
quarto preview
quarto render
cd ..
git add .
git commit -m "Initial Quarto site"
git remote set-url origin https://github.com/rferreira13/time-series.git
git subtree push --prefix myblog/_site origin gh-pages




